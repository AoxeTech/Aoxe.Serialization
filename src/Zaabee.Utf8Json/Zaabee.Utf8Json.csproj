    <ItemGroup>
        <PackageReference Include="Utf8Json" Version="1.3.7" />
        <PackageReference Include="Zaabee.Extensions" Version="2021.3.6" />
        <PackageReference Include="Zaabee.Serializer.Abstractions" Version="2021.6.0" />
    </ItemGroup>

</Project>
