<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net6.0;net7.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2022.17.0</PackageVersion>
        <Version>2022.17.0</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageProjectUrl>https://github.com/PicoHex/Zaabee.Serializers/tree/master/src/Zaabee.MsgPack</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Description>The wrappers and extensions methods for MsgPack.Cli</Description>
        <PackageTags>Zaabee;MsgPack;MsgPack.Cli;serialize;deserialize</PackageTags>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="MsgPack.Cli" Version="1.0.1" />
        <PackageReference Include="Zaabee.Extensions" Version="2022.9.0" />
        <PackageReference Include="Zaabee.Serializer.Abstractions" Version="2022.15.0" />
    </ItemGroup>

</Project>
