<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net6.0;net7.0;net8.0</TargetFrameworks>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="MemoryPack" Version="1.9.13" Condition="'$(TargetFramework)'!='netstandard2.0'" />
		<PackageReference Include="MessagePack.Annotations" Version="2.5.108" />
		<PackageReference Include="protobuf-net.Core" Version="3.2.26" />
		<PackageReference Include="xunit.assert" Version="2.4.2" />
		<PackageReference Include="Zaabee.Extensions" Version="2023.1.2" />
		<PackageReference Include="ZeroFormatter" Version="1.6.4" Condition="'$(TargetFramework)' == 'netstandard2.0'" />
		<PackageReference Include="Alexinea.ZeroFormatter" Version="1.6.4" Condition="'$(TargetFramework)' != 'netstandard2.0'" />
	</ItemGroup>
</Project>