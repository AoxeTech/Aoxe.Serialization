        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2.2.0</PackageVersion>
        <Version>2.2.0</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
